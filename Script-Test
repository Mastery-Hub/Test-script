repeat wait() until game:IsLoaded() and game.Players.LocalPlayer:FindFirstChild("PlayerGui")
local TweenService = game:GetService("TweenService")
    local Players = game:GetService("Players")
    local SoundService = game:GetService("SoundService")
    local player = Players.LocalPlayer

    -- Create GUI elements
    local MasteryHub = Instance.new("ScreenGui")
    local Blur = Instance.new("Frame")
    local MasteryHubImage = Instance.new("ImageLabel")
    local UICorner = Instance.new("UICorner")
    local MasteryHubLabel = Instance.new("TextLabel")
    local Creator = Instance.new("TextLabel")
    local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")

    -- Setup ScreenGui
    MasteryHub.Name = "MasteryHub"
    MasteryHub.Parent = player:WaitForChild("PlayerGui")
    MasteryHub.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    MasteryHub.ResetOnSpawn = false

    -- Setup Blur Frame
    Blur.Name = "Blur"
    Blur.Parent = MasteryHub
    Blur.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Blur.BackgroundTransparency = 1 -- Start invisible
    Blur.BorderSizePixel = 0
    Blur.Position = UDim2.new(0, 0, -0.153, 0)
    Blur.Size = UDim2.new(1, 0, 1.251, 0)

    -- Setup Image
    MasteryHubImage.Name = "MasteryHubImage"
    MasteryHubImage.Parent = Blur
    MasteryHubImage.AnchorPoint = Vector2.new(0.5, 0.5)
    MasteryHubImage.BackgroundTransparency = 1
    MasteryHubImage.BorderSizePixel = 0
    MasteryHubImage.Position = UDim2.new(0.5, 0, 0.308, 0)
    MasteryHubImage.Size = UDim2.new(0.15, 0, 0.316, 0)
    MasteryHubImage.Image = "http://www.roblox.com/asset/?id=103691718434653"
    MasteryHubImage.ImageTransparency = 1 -- Start transparent

    UICorner.Parent = MasteryHubImage

    -- Setup Labels
    MasteryHubLabel.Name = "MasteryHubLabel"
    MasteryHubLabel.Parent = MasteryHubImage
    MasteryHubLabel.AnchorPoint = Vector2.new(0.5, 0)
    MasteryHubLabel.BackgroundTransparency = 1
    MasteryHubLabel.Position = UDim2.new(0.5, 0, 1.1, 0)
    MasteryHubLabel.Size = UDim2.new(2, 0, 0.5, 0)
    MasteryHubLabel.Font = Enum.Font.FredokaOne
    MasteryHubLabel.Text = "Lonely Hub"
    MasteryHubLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
    MasteryHubLabel.TextScaled = true
    MasteryHubLabel.TextWrapped = true
    MasteryHubLabel.TextTransparency = 1 -- Start transparent

    Creator.Name = "Creator"
    Creator.Parent = MasteryHubImage
    Creator.AnchorPoint = Vector2.new(0.5, 0)
    Creator.BackgroundTransparency = 1
    Creator.Position = UDim2.new(0.5, 0, 1.7, 0)
    Creator.Size = UDim2.new(3, 0, 0.4, 0)
    Creator.Font = Enum.Font.FredokaOne
    Creator.Text = "Script fix lag | Made By Qminh(kenny)"
    Creator.TextColor3 = Color3.fromRGB(0, 0, 0)
    Creator.TextScaled = true
    Creator.TextWrapped = true
    Creator.TextTransparency = 1 -- Start transparent

    -- Aspect Ratio
    UIAspectRatioConstraint.Parent = MasteryHub
    UIAspectRatioConstraint.AspectRatio = 2.845

    -- Fade In Animation
    TweenService:Create(
        Blur,
        TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
        {
            BackgroundTransparency = 0.4
        }
    ):Play()

    TweenService:Create(MasteryHubImage, TweenInfo.new(1), {ImageTransparency = 0}):Play()
    TweenService:Create(MasteryHubLabel, TweenInfo.new(1), {TextTransparency = 0}):Play()
    TweenService:Create(Creator, TweenInfo.new(1), {TextTransparency = 0}):Play()

    -- Fade Out after delay
    task.delay(
        4,
        function()
            TweenService:Create(
                Blur,
                TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.In),
                {
                    BackgroundTransparency = 1
                }
            ):Play()

            TweenService:Create(MasteryHubImage, TweenInfo.new(1), {ImageTransparency = 1}):Play()
            TweenService:Create(MasteryHubLabel, TweenInfo.new(1), {TextTransparency = 1}):Play()
            TweenService:Create(Creator, TweenInfo.new(1), {TextTransparency = 1}):Play()

            task.delay(
                1,
                function()
                    MasteryHub:Destroy()
                end
            )
        end
    )
-- Script t·∫°o b·ªüi Qminh(kenny)

